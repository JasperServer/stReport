	<queryString language="SQL">
		<![CDATA[SELECT
	MAX (
		CASE note
		WHEN '货运收入' THEN

		VALUE

		ELSE
			0
		END
	) AS
VALUE
	,
	ID,
	MAX (
		CASE note
		WHEN '基准' THEN

		VALUE

		ELSE
			0
		END
	) AS base,
	'货运收入' AS note
FROM
	(
		SELECT
			entry052 AS
		VALUE
			,
			'freightIncome' AS ID,
			'货运收入' AS note
		FROM
			lj18.data_date
		WHERE
			d18_date = to_char(CURRENT_DATE - 1, 'YYYYMMDD')
		AND report_name = 'CB2'
		AND unit = 'XLJ'
		UNION ALL
			SELECT
				entry052 * 2 AS base,
				'freightIncome' AS ID,
				'基准' AS note
			FROM
				lj18.data_date
			WHERE
				d18_date = to_char(CURRENT_DATE - 2, 'YYYYMMDD')
			AND report_name = 'CB2'
			AND unit = 'XLJ'
	) AS P
GROUP BY
	ID
UNION ALL
	SELECT
		MAX (
			CASE note
			WHEN '发送吨数' THEN

			VALUE

			ELSE
				0
			END
		) AS
	VALUE
		,
		ID,
		MAX (
			CASE note
			WHEN '基准' THEN

			VALUE

			ELSE
				0
			END
		) AS base,
		'发送吨数' AS note
	FROM
		(
			SELECT
				entry003 AS
			VALUE
				,
				'freightTonnage' AS ID,
				'发送吨数' AS note
			FROM
				lj18.data_date
			WHERE
				d18_date = to_char(CURRENT_DATE - 1, 'YYYYMMDD')
			AND report_name = 'XB2'
			AND unit = 'XLJ'
			UNION ALL
				SELECT
					entry003 * 2 AS base,
					'freightTonnage' AS ID,
					'基准' AS note
				FROM
					lj18.data_date
				WHERE
					d18_date = to_char(CURRENT_DATE - 2, 'YYYYMMDD')
				AND report_name = 'XB2'
				AND unit = 'XLJ'
		) AS P
	GROUP BY
		ID
	UNION ALL
		SELECT
			MAX (
				CASE note
				WHEN '生产指数' THEN

				VALUE

				ELSE
					0
				END
			) AS
		VALUE
			,
			ID,
			MAX (
				CASE note
				WHEN '基准' THEN

				VALUE

				ELSE
					0
				END
			) AS base,
			'生产指数' AS note
		FROM
			(
				SELECT
					round(
						(
							((jj / zong) * (jj_current / jj_rj) + (ke / zong) * (ke_current / ke_rj) + (huo / zong) * (huo_current / huo_rj))/100
						),
						3
					) AS
				VALUE
					,
					'productIndex' AS ID,
					'生产指数' AS note
				FROM
					(
						SELECT
							MAX (
								CASE bz
								WHEN '总收入' THEN
									ndys
								ELSE
									0
								END
							) AS zong,
							MAX (
								CASE bz
								WHEN '客运收入' THEN
									ndys
								ELSE
									0
								END
							) AS ke,
							MAX (
								CASE bz
								WHEN '货运收入' THEN
									ndys
								ELSE
									0
								END
							) AS huo,
							MAX (
								CASE bz
								WHEN '建设基金' THEN
									ndys
								ELSE
									0
								END
							) AS jj,
							MAX (
								CASE bz
								WHEN '客运收入' THEN
									rjys
								ELSE
									0
								END
							) AS ke_rj,
							MAX (
								CASE bz
								WHEN '货运收入' THEN
									rjys
								ELSE
									0
								END
							) AS huo_rj,
							MAX (
								CASE bz
								WHEN '建设基金' THEN
									rjys
								ELSE
									0
								END
							) AS jj_rj,
							AVG (entry049) AS ke_current,
							AVG (entry052) AS huo_current,
							AVG (entry055) AS jj_current
						FROM
							lj18.data_date dd,
							basic_ndysb bn
						WHERE
							d18_date = to_char(CURRENT_DATE - 1, 'YYYYMMDD')
						AND report_name = 'CB2'
						AND unit = 'XLJ'
						AND dd.unit = bn.unit_code
					) AS P
				UNION ALL
					SELECT
						round(
							(
								((jj / zong) * (jj_current / jj_rj) + (ke / zong) * (ke_current / ke_rj) + (huo / zong) * (huo_current / huo_rj))/50
							),
							3
						) AS
					VALUE
						,
						'productIndex' AS ID,
						'基准' AS note
					FROM
						(
							SELECT
								MAX (
									CASE bz
									WHEN '总收入' THEN
										ndys
									ELSE
										0
									END
								) AS zong,
								MAX (
									CASE bz
									WHEN '客运收入' THEN
										ndys
									ELSE
										0
									END
								) AS ke,
								MAX (
									CASE bz
									WHEN '货运收入' THEN
										ndys
									ELSE
										0
									END
								) AS huo,
								MAX (
									CASE bz
									WHEN '建设基金' THEN
										ndys
									ELSE
										0
									END
								) AS jj,
								MAX (
									CASE bz
									WHEN '客运收入' THEN
										rjys
									ELSE
										0
									END
								) AS ke_rj,
								MAX (
									CASE bz
									WHEN '货运收入' THEN
										rjys
									ELSE
										0
									END
								) AS huo_rj,
								MAX (
									CASE bz
									WHEN '建设基金' THEN
										rjys
									ELSE
										0
									END
								) AS jj_rj,
								AVG (entry049) AS ke_current,
								AVG (entry052) AS huo_current,
								AVG (entry055) AS jj_current
							FROM
								lj18.data_date dd,
								basic_ndysb bn
							WHERE
								d18_date = to_char(CURRENT_DATE - 2, 'YYYYMMDD')
							AND report_name = 'CB2'
							AND unit = 'XLJ'
							AND dd.unit = bn.unit_code
						) AS p2
			) AS p4
		GROUP BY
			ID
		UNION ALL
			SELECT
				MAX (
					CASE note
					WHEN '客运收入' THEN

					VALUE

					ELSE
						0
					END
				) AS
			VALUE
				,
				ID,
				MAX (
					CASE note
					WHEN '基准' THEN

					VALUE

					ELSE
						0
					END
				) AS base,
				'客运收入' AS note
			FROM
				(
					SELECT
						entry049 AS
					VALUE
						,
						'passengerIncome' AS ID,
						'客运收入' AS note
					FROM
						lj18.data_date
					WHERE
						d18_date = to_char(CURRENT_DATE - 1, 'YYYYMMDD')
					AND report_name = 'CB2'
					AND unit = 'XLJ'
					UNION ALL
						SELECT
							entry049 * 2 AS base,
							'passengerIncome' AS ID,
							'基准' AS note
						FROM
							lj18.data_date
						WHERE
							d18_date = to_char(CURRENT_DATE - 2, 'YYYYMMDD')
						AND report_name = 'CB2'
						AND unit = 'XLJ'
				) AS P
			GROUP BY
				ID
			UNION ALL
				SELECT
					MAX (
						CASE note
						WHEN '客运人数' THEN

						VALUE

						ELSE
							0
						END
					) AS
				VALUE
					,
					ID,
					MAX (
						CASE note
						WHEN '基准' THEN

						VALUE

						ELSE
							0
						END
					) AS base,
					'客运人数' AS note
				FROM
					(
						SELECT
							entry002 AS
						VALUE
							,
							'passengerNumber' AS ID,
							'客运人数' AS note
						FROM
							lj18.data_date
						WHERE
							d18_date = to_char(CURRENT_DATE - 1, 'YYYYMMDD')
						AND report_name = 'SBSC'
						AND unit = 'XLJ'
						UNION ALL
							SELECT
								entry002 * 2 AS base,
								'passengerNumber' AS ID,
								'基准' AS note
							FROM
								lj18.data_date
							WHERE
								d18_date = to_char(CURRENT_DATE - 2, 'YYYYMMDD')
							AND report_name = 'SBSC'
							AND unit = 'XLJ'
					) AS P
				GROUP BY
					ID]]>
	</queryString>
