	<queryString>
		<![CDATA[select 
 max(case year when 'thisYear' then thisyear else 0 end) as thisyear,
max(case year when 'lastYear' then thisYear else 0 end) as lastyear,
dates as date
from(
select round(entry002/10000,0) as thisYear,'thisYear' as year,to_char(to_date(d18_date,'YYYYmmdd'),'mm-dd') as dates
from lj18.data_date
where d18_date >= to_char(CURRENT_DATE-8, 'YYYYMMDD') and d18_date <= to_char(CURRENT_DATE-1, 'YYYYMMDD') and report_name = 'SBSC' and unit = 'XLJ'
union all
select round(entry002/10000,0) as lastYear,'lastYear' as year,to_char(to_date(d18_date,'YYYYmmdd'),'mm-dd') as dates
from lj18.data_date
where d18_date >= to_char((now()::timestamp + '-1 year 0 month -8 day '),'yyyymmdd') and d18_date <= to_char((now()::timestamp + '-1 year 0 month -1 day '),'yyyymmdd') and report_name = 'SBSC' and unit = 'XLJ'
) as p
group by dates  order by  dates]]>
	</queryString>
